{
  "name": "afk-timer",
  "productName": "AFK Timer",
  "version": "0.0.1",
  "description": "A minimal Electron countdown timer.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac --win"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Drew Bloomfield",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-packager": "^12.1.1",
    "electron-builder": "^20.28.3"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "moment": "^2.22.2"
  },
  "build": {
    "appId": "AFKTimer",
    "productName": "AFK Timer",
    "mac": {
      "category": "timing"
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  }
}
